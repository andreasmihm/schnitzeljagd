<?xml version='1.0' encoding='utf-8'?>
<widget
    id="com.mihm.schnitzeljagd"
    version="1.0.0"
    xmlns:gap="http://phonegap.com/ns/1.0"
    xmlns="http://www.w3.org/ns/widgets"
    xmlns:cdv="http://cordova.apache.org/ns/1.0">
 
    <name>Schnitzeljagd</name>
    <description>
        Schnitzeljagd
    </description>
    <author email="andreas.mihm@gmx.de" href="http://www.me.me">
        Andreas Mihm
    </author>
	
	<plugin name="cordova-plugin-media" version="2.3.0" />
	
  <platform name="ios">
        <!-- iOS 8.0+ -->
        <icon src="ios/AppIcon.appiconset/Icon-App-60x60@3x.png" width="180" height="180" />
        <!-- iOS 7.0+ -->
        <!-- iPhone / iPod Touch  -->
        <icon src="ios/AppIcon.appiconset/Icon-App-60x60@1x.png" width="60" height="60" />
        <icon src="ios/AppIcon.appiconset/Icon-App-60x60@2x.png" width="120" height="120" />
        <!-- iPad -->
        <icon src="ios/AppIcon.appiconset/Icon-App-76x76@1x.png" width="76" height="76" />
        <icon src="ios/AppIcon.appiconset/Icon-App-76x76@2x.png" width="152" height="152" />
        <!-- iOS 6.1 -->
        <!-- Spotlight Icon -->
        <icon src="ios/AppIcon.appiconset/Icon-App-40x40@1x.png" width="40" height="40" />
        <icon src="ios/AppIcon.appiconset/Icon-App-40x40@2x.png" width="80" height="80" />
        <!-- iPhone / iPod Touch -->
        <icon src="ios/AppIcon.appiconset/Icon-App-60x60@1x.png" width="60" height="60" />
        <icon src="ios/AppIcon.appiconset/Icon-App-60x60@2x.png" width="120" height="120" />
        <!-- iPad -->
        <icon src="ios/AppIcon.appiconset/Icon-App-72x72@2x.png" width="72" height="72" />
        <icon src="ios/AppIcon.appiconset/Icon-App-72x72@2x.png" width="144" height="144" />
        <!-- iPhone Spotlight and Settings Icon -->
        <icon src="ios/AppIcon.appiconset/Icon-App-29x29@1x.png" width="29" height="29" />
        <icon src="ios/AppIcon.appiconset/Icon-App-29x29@2x.png" width="58" height="58" />
        <!-- iPad Spotlight and Settings Icon -->
        <icon src="ios/AppIcon.appiconset/Icon-App-Small-50x50@1x.png" width="50" height="50" />
        <icon src="ios/AppIcon.appiconset/Icon-App-Small-50x50@2x.png" width="100" height="100" />
    </platform>
 
    <platform name="android">
        <icon src="android/mipmap-mdpi/ic_launcher.png" density="mdpi" />
        <icon src="android/mipmap-hdpi/ic_launcher.png" density="hdpi" />
        <icon src="android/mipmap-xhdpi/ic_launcher.png" density="xhdpi" />
        <icon src="android/mipmap-xxhdpi/ic_launcher.png" density="xxhdpi" />
        <icon src="android/mipmap-xxxhdpi/ic_launcher.png" density="xxxhdpi" />
    </platform>
    
	<access origin="*" />
 
    <preference name="EnableDeveloperConsole" value="true"/>
</widget>